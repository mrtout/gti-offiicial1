# Configuration API de vols (Skyscanner/Mystifly)
NEXT_PUBLIC_FLIGHT_API_URL=https://api.skyscanner.net/v1
NEXT_PUBLIC_FLIGHT_API_KEY=your_flight_api_key_here

# Configuration SendGrid pour les emails
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=noreply@groupetanouinternational.com

# Configuration Twilio pour les SMS
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Configuration base de données (PostgreSQL)
DATABASE_URL=postgresql://username:password@localhost:5432/groupe_tanou_db

# Configuration Redis pour le cache
REDIS_URL=redis://localhost:6379

# Configuration paiement
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Configuration Mobile Money
ORANGE_MONEY_API_KEY=your_orange_money_api_key_here
MOOV_MONEY_API_KEY=your_moov_money_api_key_here
WAVE_API_KEY=your_wave_api_key_here

# Clé de chiffrement pour les données sensibles
ENCRYPTION_KEY=your_32_character_encryption_key_here

# URL de base de l'application
NEXT_PUBLIC_APP_URL=https://groupetanouinternational.com